; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Power11mobile"
run/main_scene="res://ui/MainMenu.tscn"
config/features=PackedStringArray("4.5", "GL Compatibility")
config/icon="res://assets/game_icon_padded.png"
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false

[audio]

driver/mix_rate=96000

[autoload]

Settings="*res://core/Settings.gd"
SoundManager="*res://core/SoundManager.gd"
FontLoader="*res://core/FontLoader.gd"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
display/window/size/viewport_width=720
display/window/size/viewport_height=1280
display/window/stretch/mode="viewport"
display/window/stretch/aspect="expand"
display/window/handheld/orientation=1
